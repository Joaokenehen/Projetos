create table itens (
		id_item INT,
		nome_item VARCHAR(20),
		item_descricao VARCHAR(90),
		item_quantidade INT
		);
	
	
	
insert itens values (1, "Chave", "Uma chave dourada, talvez possa ser usada para abrir algum lugar", 1),
